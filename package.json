{
  "author": "Leonardo Meireles",
  "dependencies": {
    "dotenv": "^16.5.0",
    "winston": "^3.17.0"
  },
  "description": "TCP Proxy for Laboratory Equipment Communication with ASTM/HL7 Support",
  "devDependencies": {
    "prettier": "^2.8.8"
  },
  "keywords": [
    "laboratory",
    "proxy",
    "astm",
    "hl7",
    "tcp",
    "medical"
  ],
  "license": "ISC",
  "main": "app.js",
  "name": "proxy_for_labs",
  "scripts": {
    "start": "node app.js",
    "start:emu": "node start-emu-environment.js",
    "start:system": "node start-system.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:startup": "pm2 startup",
    "pm2:save": "pm2 save",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write",
    "stop": "pkill -f 'node app.js|node start-emu-environment.js'",
    "stop:all": "pkill node",
    "check:ports": "lsof -i :54000 && lsof -i :7006 && lsof -i :8000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "commonjs",
  "version": "1.0.0"
}
